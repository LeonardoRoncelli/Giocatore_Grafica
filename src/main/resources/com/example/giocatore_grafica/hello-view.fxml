<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="517.0" prefWidth="655.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.giocatore_grafica.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
   <AnchorPane prefHeight="486.0" prefWidth="652.0">
      <children>
         <Button fx:id="aggiungi" layoutX="41.0" layoutY="14.0" mnemonicParsing="false" onAction="#onaggiungiClick" text="Aggiungi" />
         <Button fx:id="modifica" layoutX="116.0" layoutY="14.0" mnemonicParsing="false" text="Modifica" />
         <Button fx:id="cancella" layoutX="190.0" layoutY="14.0" mnemonicParsing="false" text="Cancella" />
         <Button fx:id="maggiore" layoutX="262.0" layoutY="14.0" mnemonicParsing="false" text="Maggiore di 5" />
         <TextField fx:id="t1" layoutX="41.0" layoutY="77.0" onAction="#onaggiungiClick" />
         <TextField fx:id="t2" layoutX="41.0" layoutY="131.0" />
         <TextField fx:id="t3" layoutX="41.0" layoutY="179.0" />
         <Label layoutX="41.0" layoutY="60.0" prefHeight="17.0" prefWidth="114.0" text="Nome giocatore" />
         <Label layoutX="41.0" layoutY="114.0" text="Capitano" />
         <Label layoutX="41.0" layoutY="162.0" text="Goal" />
      </children>
   </AnchorPane>
</VBox>
